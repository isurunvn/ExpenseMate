<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.demo.Controllers.AddShopController">
    <Label text="Add Shop" />
    <TextField fx:id="shopNameField" promptText="Shop Name" />
    <TextField fx:id="shopAddressField" promptText="Shop Location" />
    <Button text="Add Shop" onAction="#handleAddShop" />
</VBox>